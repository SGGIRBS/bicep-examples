{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "databaseAccounts_mongotest53456456_name": {
            "defaultValue": "mongotest53456456",
            "type": "String"
        },
        "privateEndpoints_cosmosDbPe_externalid": {
            "defaultValue": "/subscriptions/2cc9b477-4a83-46de-b99f-3253917afef2/resourceGroups/unite_poc-rg/providers/Microsoft.Network/privateEndpoints/cosmosDbPe",
